e, \"email\": email, \"agent\": agent, \"comment\": comment,\r\n        headers: { 'Content-Type': 'application/json',\r\n                    'Access-Control-Allow-Origin' : '*',\r\n                    'Access-Control-Allow-Methods': 'DELETE, POST, GET, OPTIONS',\r\n                    'Access-Control-Allow-Headers': 'Content-Type, Access-Control-Allow-Headers, Authorization, X-Requested-With'}\r\n      }\r\n    )\r\n    console.log(response.data)\r\n\r\n  }\r\n  \r\n  function submitContactForm(name, email, agent, comment) {\r\n    setModalIsOpen(false);\r\n    sendEmail(name, email, agent, comment);\r\n  }\r\n\r\n\r\n  return (\r\n    <div class=\"main\">\r\n\r\n      <button class='modal-button' onClick = {() => setModalIsOpen(true)}> Contact Us  </button>\r\n      <Modal isOpen = {modalIsOpen}> \r\n\r\n        <label> Name: </label>\r\n        <input  type=\"text\"  value={name} onInput={e => setName(e.target.value)}/>\r\n        <br></br>\r\n\r\n        <label> Email: </label>\r\n        <input type=\"email\"  value={email} onInput={e => setEmail(e.target.value)}/> \r\n        <br></br>\r\n\r\n        <label> Select Agents: </label>\r\n        <select name ='selectAgents'  value={agent} onChange={e => setAgent(e.target.value)} >\r\n\r\n          <option value=\"JACOB\">JACOB</option>\r\n          <option value=\"saab\">Saab</option>\r\n          <option value=\"opel\">Opel</option>\r\n          <option value=\"audi\">Audi</option>\r\n\r\n        </select> \r\n\r\n        <br></br>\r\n        \r\n        <label> Comment: </label> \r\n        <textarea  value={comment} onInput={e => setComment(e.target.value)}>  </textarea>\r\n        \r\n        <br></br>\r\n\r\n        <button class=\"submit-button\" onClick = { () => submitContactForm(name, email, agent, comment)}> Submit</button>\r\n\r\n      </Modal>\r\n        \r\n      <nav class = \"navigation\">\r\n        <button class=\"logo-button\" onClick={scrollToLandingPage}>\r\n          <img class=\"logo\" src={ logo } alt=\"logo\" />\r\n        </button>\r\n        <button class=\"navigation-button\" onClick={scrollToBuyAHome}>\r\n          <span class=\"\">Buy a Home</span>\r\n        </button> \r\n        <button class=\"navigation-button\" onClick={scrollToSellAHome}>\r\n          <span class=\"\">Sell a Home</span>\r\n        </button>\r\n        <button class=\"navigation-button\" onClick={scrollToAboutUs}>\r\n          <span class=\"\">About Us</span>\r\n        </button>\r\n      </nav>\r\n\r\n      {/* <img src={niceHouse} alt=\"A nice home\" class=\"landing-page-image\"/> */}\r\n      <div class=\"background\"> </div>\r\n\r\n      <div class=\"content\">\r\n        <div ref={landingPageRef}>\r\n          <LandingPage />\r\n        </div>\r\n        <div class=\"page\" ref={buyAHomeRef}>\r\n          <BuyAHome />\r\n        </div>\r\n        <div class=\"page\" ref={sellAHomeRef}>\r\n          <SellAHome />\r\n        </div>\r\n        <div class=\"page\" ref={aboutUsRef}>\r\n          <AboutUs />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\Owner\\Documents\\Projects\\WRE\\WRE-website\\src\\LandingPage\\LandingPage.js",[],"C:\\Users\\Owner\\Documents\\Projects\\WRE\\WRE-website\\src\\BuyAHome\\BuyAHome.js",[],"C:\\Users\\Owner\\Documents\\Projects\\WRE\\WRE-website\\src\\SellAHome\\SellAHome.js",[],"C:\\Users\\Owner\\Documents\\Projects\\WRE\\WRE-website\\src\\AboutUs\\AboutUs.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":4,"column":8,"nodeType":"41","messageId":"42","endLine":4,"endColumn":17},"no-native-reassign",["43"],"no-negated-in-lhs",["44"],"no-unused-vars","'niceHouse' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]